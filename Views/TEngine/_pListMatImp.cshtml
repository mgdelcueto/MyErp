@model List<TExplosionB>
    <table class="table-norm" border="1">
    <div class="cont-titles">
    </div>
    <tr>
        <th class="head-text">Level</th> 
        <th class="head-text">Reference</th>                
        <th class="head-text">Description</th>                
        <th class="head-text">Quantity</th>                
        <th class="head-text">UM</th>                
        <th class="head-text">Fase</th>                
        <th class="head-text">Operation</th>                
        <th class="head-text">Time</th>                
        <th class="head-text">TUnit</th>                
    </tr>
   
    @foreach (var c in Model) {
        <tr>
            <td class ="col-text">@c.ExpsLevel</td>            
            <td class ="norm-text">@c.ExpRefer</td>
            <td class ="norm-text">@c.ExpDescr</td>
            <td class ="norm-text">@c.ExpCoef</td>
            <td class ="norm-text">@c.ExpUm</td>
            <td class ="norm-text">@c.ExpRouFase</td>
            <td class ="norm-text">@c.ExpRouOper</td>
            <td class ="norm-text">@c.ExpRouTime</td>
            <td class ="norm-text">@c.ExpRouTunit</td>
            <td class="col-text">@Html.ActionLink("Edit", "MatEdit", new { id = c.ExpComp ,MId= c.ExpComp,panel=4})</td>
            
        </tr>
    }
    </table>

